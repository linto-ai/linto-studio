{
  "/auth/login": {
    "post": {
      "tags": [
        "auth"
      ],
      "summary": "Authenticate user",
      "requestBody": {
        "content": {
          "multipart/form-data": {
            "schema": {
              "type": "object",
              "properties": {
                "email": {
                  "type": "string"
                },
                "password": {
                  "type": "string"
                }
              },
              "required": [
                "email",
                "password"
              ]
            }
          }
        }
      },
      "responses": {
        "200": {
          "description": "OK"
        }
      }
    }
  }
}