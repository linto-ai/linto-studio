{
  "/api/organizations/": {
    "post": {
      "tags": [
        "organizations"
      ],
      "summary": "Create an organization",
      "requestBody": {
        "content": {
          "multipart/form-data": {
            "schema": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string"
                },
                "type": {
                  "type": "string",
                  "enum": [
                    "public",
                    "private"
                  ]
                }
              },
              "required": [
                "name"
              ]
            }
          }
        }
      },
      "responses": {
        "201": {
          "description": "OK",
          "content": {
            "application/json": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string",
                  "description": "Organization xxxxxx created"
                },
                "organizationId": {
                  "type": "string",
                  "description": "Organization id"
                }
              },
              "example": {
                "message": "Organization test-norg-user created",
                "organizationId": "639312d5b36cdb549eb1deab"
              }
            }
          }
        },
        "401": {
          "$ref": "#/components/responses/Unauthorized"
        },
        "409": {
          "$ref": "#/components/responses/Conflict"
        },
        "415": {
          "$ref": "#/components/responses/UnsupportedMediaType"
        }
      }
    }
  },
  "/api/organizations/user": {
    "get": {
      "tags": [
        "organizations"
      ],
      "summary": "List user organizations where the user is a member",
      "responses": {
        "200": {
          "description": "OK",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "userOrganizations": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "$ref": "#/components/schemas/OrganizationReduce"
                    }
                  }
                }
              }
            }
          }
        },
        "401": {
          "$ref": "#/components/responses/Unauthorized"
        }
      }
    }
  },
  "/api/organizations/search": {
    "post": {
      "tags": [
        "organizations"
      ],
      "summary": "Search organizations by name",
      "requestBody": {
        "content": {
          "multipart/form-data": {
            "schema": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string"
                }
              },
              "required": [
                "name"
              ]
            }
          }
        }
      },
      "responses": {
        "200": {
          "description": "OK",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "organization": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "$ref": "#/components/schemas/OrganizationReduce"
                    }
                  }
                }
              }
            }
          }
        },
        "401": {
          "$ref": "#/components/responses/Unauthorized"
        },
        "415": {
          "$ref": "#/components/responses/UnsupportedMediaType"
        }
      }
    }
  }
}