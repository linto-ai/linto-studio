      "requestBody": {
        "content": {
          "multipart/form-data": {
            "schema": {
              "type": "object",
              "properties": {
                "email": {
                  "type": "string"
                },
                "role": {
                  "type": "string"
                }
              },
              "required": [
                "email",
                "role"
              ]
            }
          }
        }
      },
      "responses": {
        "200": {
          "description": "OK"
        }
      }
