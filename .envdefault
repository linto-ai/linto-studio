COMPONENTS=WebServer
WEBSERVER_HTTP_PORT=80

# Mongo
DB_HOST=localhost
DB_PORT=27017
DB_REQUIRE_LOGIN=true
DB_USER=root
DB_PASS=example
DB_NAME=conversations

# STT Config
TRANSCRIPTION_SERVICES=[{"host":"<transcription_host>","lang":"fr-FR","name":"transcription_service_name", "desc":"some description", "model_quality" : 4, "accoustic" : 2}]

STT_REQUIRE_AUTH=false
STT_USER=stt_auth
STT_PASSWORD=stt_password

# NLP Service
NLP_SERVICES=<nlp_service_host>
NLP_METHOD=frequencies,textrank

# Cross domain accessibility
CORS_ENABLED=false
CORS_API_WHITELIST=

# Express settings
EXPRESS_SIZE_FILE_MAX=500mb
AXIOS_SIZE_FILE_MAX=500000000

# JWT SECRET
CM_JWT_SECRET=jwt_secret
CM_REFRESH_SECRET=jwt_refresh_secret