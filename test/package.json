{
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cypress": "^13.7.2",
    "cypress-real-events": "^1.12.0",
    "cypress-terminal-report": "^6.0.1"
  },
  "scripts": {
    "e2e:electron": "cypress run --browser electron",
    "cy:first": "cypress run --browser electron --config-file cypress.first-user.config.js",
    "cy:second": "cypress run --browser electron --config-file cypress.second-user.config.js",
    "cy:third": "cypress run --browser electron --config-file cypress.third-user.config.js",
    "cy:collab": "concurrently \"npm run cy:first\" \"npm run cy:second\" \"npm run cy:third\""
  }
}
