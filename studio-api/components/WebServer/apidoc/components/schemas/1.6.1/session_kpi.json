{
  "SessionKPI": {
    "type": "object",
    "properties": {
      "userCount": {
        "type": "object",
        "properties": {
          "total": {
            "type": "integer",
            "description": "Total number of users in the session"
          },
          "totalConnections": {
            "type": "integer",
            "description": "Total number of connections across all users"
          },
          "above5Min": {
            "type": "integer",
            "description": "Number of users who watched more than 5 minutes"
          },
          "below5Min": {
            "type": "integer",
            "description": "Number of users who watched less than 5 minutes"
          }
        }
      },

      "sessionInfo": {
        "type": "object",
        "properties": {
          "sessionId": {
            "type": "string",
            "description": "ID of the session"
          },
          "name": {
            "type": "string",
            "description": "Name of the session"
          },
          "organizationId": {
            "type": "string",
            "description": "ID of the associated organization"
          },
          "visibility": {
            "type": "string",
            "description": "Session visibility (public, private, organization)"
          }
        }
      },

      "organization": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "Organization ID"
          },
          "info": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string",
                "description": "Organization name"
              },
              "description": {
                "type": ["string", "null"],
                "description": "Description of the organization"
              }
            }
          }
        }
      },

      "totalWatchTimeSeconds": {
        "type": "number",
        "description": "Total watch time for the session in seconds"
      },

      "totalWatchTimeMinutes": {
        "type": "number",
        "description": "Total watch time for the session in minutes"
      },

      "totalWatchTimeHours": {
        "type": "number",
        "description": "Total watch time for the session in hours"
      },

      "avgWatchTimeSeconds": {
        "type": "number",
        "description": "Average watch time per user in seconds"
      },

      "watchTime": {
        "type": "object",
        "properties": {
          "avgAbove5MinSeconds": {
            "type": "number",
            "description": "Average watch time in seconds for users above 5 minutes"
          },
          "avgBelow5MinSeconds": {
            "type": "number",
            "description": "Average watch time in seconds for users below 5 minutes"
          },
          "avgAbove5MinMinutes": {
            "type": "number",
            "description": "Average watch time in minutes for users above 5 minutes"
          },
          "avgBelow5MinMinutes": {
            "type": "number",
            "description": "Average watch time in minutes for users below 5 minutes"
          }
        }
      }
    },

    "example": {
      "userCount": {
        "total": 5,
        "totalConnections": 7,
