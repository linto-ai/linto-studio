{
  "TranscriberProfileRequest": {
    "type": "object",
    "properties": {
      "config": {
        "$ref": "#/components/schemas/TranscriberProfileData"
      },
      "organizationId": {
        "type": "string",
        "default": null
      },
      "quickMeeting": {
        "type": "boolean",
        "default": null
      },
      "meta": {
        "type": "object",
        "description": "Custom metadata for the transcriber profile",
        "default": null
      }
    },
    "required": ["config"]
  },
  "TranscriberProfileAnswer": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer",
        "description": "The ID of the transcriber profile"
      },
      "config": {
        "allOf": [
          { "$ref": "#/components/schemas/TranscriberProfileData" },
          {
            "type": "object",
            "properties": {
              "availableTranslations": {
                "type": "array",
                "description": "The available translations with this ASR",
                "items": {
                  "type": "string"
                }
              },
              "hasDiarization": {
                "description": "The ASR supports diarization",
                "type": "boolean"
              }
            }
          }
        ]
      },
      "organizationId": {
        "type": "string",
        "description": "Organization ID of the transcriber profile"
      },
      "quickMeeting": {
        "type": "boolean",
        "description": "True if this transcriber profile is available for a quick meeting"
      },
      "meta": {
        "type": "object",
        "description": "Custom metadata for the transcriber profile"
      },
      "createdAt": {
        "type": "string",
        "format": "date-time",
        "description": "Creation date of the transcriber profile"
      },
      "updatedAt": {
        "type": "string",
        "format": "date-time",
        "description": "Update date of the transcriber profile"
      }
    },
    "required": ["config"]
  },
  "TranscriberProfileData": {
    "type": "object",
    "properties": {
      "type": {
        "type": "string",
        "enum": ["microsoft", "linto", "amazon"]
      },
      "name": {
        "type": "string"
      },
      "description": {
        "type": "string"
      },
      "languages": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "candidate": {
              "type": "string",
              "description": "Language code BCP 47"
            },
            "endpoint": {
              "type": "string"
            }
          },
          "required": ["candidate"]
        }
      },
      "availableTranslations": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "key": {
        "type": "string"
      },
      "region": {
        "type": "string"
      },
      "endpoint": {
        "type": "string"
      },
      "certificate": {
        "type": "string",
        "format": "binary",
        "description": "X.509 certificate file (.crt) for AWS IAM Roles Anywhere authentication"
      },
      "privateKey": {
        "type": "string",
        "format": "binary",
        "description": "Private key file (.pem) for AWS IAM Roles Anywhere authentication"
      },
      "passphrase": {
        "type": "string",
        "description": "Passphrase for the private key (if encrypted)"
      },
      "credentials": {
        "type": "string",
        "description": "Auto-generated encrypted bundle containing certificate, private key, and passphrase (read-only, set by server)"
      },
      "trustAnchorArn": {
        "type": "string",
        "description": "AWS IAM Roles Anywhere Trust Anchor ARN"
      },
      "profileArn": {
        "type": "string",
        "description": "AWS IAM Roles Anywhere Profile ARN"
      },
      "roleArn": {
        "type": "string",
        "description": "AWS IAM Role ARN to assume"
      }
    },
    "oneOf": [
      {
        "required": ["type", "name", "description", "languages", "key", "region"]
      },
      {
        "required": ["type", "name", "description", "languages", "endpoint"]
      }
    ]
  },
  "TranscriberProfileUpdateRequest": {
    "type": "object",
    "properties": {
      "config": {
        "$ref": "#/components/schemas/TranscriberProfilePartialData"
      },
      "organizationId": {
        "type": "string",
        "description": "Organization ID of the transcriber profile"
      },
      "quickMeeting": {
        "type": "boolean",
        "description": "True if this transcriber profile is available for a quick meeting"
      },
      "meta": {
        "type": "object",
        "description": "Custom metadata for the transcriber profile"
      }
    }
  },
  "TranscriberProfilePartialData": {
    "type": "object",
    "properties": {
      "type": {
        "type": "string",
        "enum": ["microsoft", "linto", "amazon"]
      },
      "name": {
        "type": "string"
      },
      "description": {
        "type": "string"
      },
      "languages": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "candidate": {
              "type": "string",
              "description": "Language code BCP 47"
            },
            "endpoint": {
              "type": "string"
            }
          }
        }
      },
      "availableTranslations": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "key": {
        "type": "string"
      },
      "region": {
        "type": "string"
      },
      "endpoint": {
        "type": "string"
      },
      "certificate": {
        "type": "string",
        "format": "binary",
        "description": "X.509 certificate file (.crt) for AWS IAM Roles Anywhere authentication"
      },
      "privateKey": {
        "type": "string",
        "format": "binary",
        "description": "Private key file (.pem) for AWS IAM Roles Anywhere authentication"
      },
      "passphrase": {
        "type": "string",
        "description": "Passphrase for the private key (if encrypted)"
      },
      "credentials": {
        "type": "string",
        "description": "Auto-generated encrypted bundle containing certificate, private key, and passphrase (read-only, set by server)"
      },
      "trustAnchorArn": {
        "type": "string",
        "description": "AWS IAM Roles Anywhere Trust Anchor ARN"
      },
      "profileArn": {
        "type": "string",
        "description": "AWS IAM Roles Anywhere Profile ARN"
      },
      "roleArn": {
        "type": "string",
        "description": "AWS IAM Role ARN to assume"
      }
    }
  }
}
