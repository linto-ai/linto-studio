        }
      ],
      "requestBody": {
        "content": {
          "multipart/form-data": {
            "schema": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string"
                },
                "sessionId": {
                  "type": "string"
                }
              },
              "required": []
            }
          }
        }
      },
      "responses": {
        "201": {
