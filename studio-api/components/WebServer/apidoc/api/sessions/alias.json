  "/api/organizations/{organizationId}/sessions/alias": {
    "get": {
      "tags": ["sessions alias"],
      "summary": "List all alias",
      "parameters": [
        {
          "in": "path",
          "name": "organizationId",
          "type": "string",
          "format": "uuid",
          "required": true,
          "description": "uuid string identifier for an organization"
        }
      ],
      "responses": {
        "200": {
          "description": "The list of session alias",
          "content": {
            "application/json": {
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/components/schemas/SessionAlias"
                }
              }
            }
          }
        }
      }
    },
    "post": {
