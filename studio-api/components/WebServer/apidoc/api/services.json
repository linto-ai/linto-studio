{
  "/api/services": {
    "get": {
      "tags": ["services"],
      "summary": "Get all runing services based of the gateway api",
      "parameters": [
        {
          "name": "securityLevel",
          "in": "query",
          "required": false,
          "description": "Filter transcription services by security level (insecure, sensitive, secure). Services without security_level are treated as insecure.",
          "schema": {
            "type": "string",
            "enum": ["insecure", "sensitive", "secure"]
          }
        }
      ],
      "responses": {
        "200": {
          "description": "The list of services",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Services"
              }
            }
          }
        },
        "401": {
          "$ref": "#/components/responses/Unauthorized"
        }
      }
    }
  },
  "/api/services/{scope}": {
    "get": {
      "tags": ["services"],
      "summary": "Get all runing services based of the gateway api",
      "parameters": [
        {
          "name": "scope",
          "in": "path",
          "required": true,
          "schema": {
            "type": "string"
          }
        },
        {
          "name": "securityLevel",
          "in": "query",
          "required": false,
          "description": "Filter transcription services by security level (insecure, sensitive, secure). Services without security_level are treated as insecure.",
          "schema": {
            "type": "string",
            "enum": ["insecure", "sensitive", "secure"]
          }
        }
      ],
      "responses": {
        "200": {
          "description": "The list of services",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Services"
              }
            }
          }
        },
        "401": {
          "$ref": "#/components/responses/Unauthorized"
        }
      }
    }
  },
  "/api/services/{organizationId}/llm": {
    "get": {
      "tags": ["services"],
      "summary": "Get all llm service listed",
      "parameters": [
        {
          "name": "organizationId",
          "in": "query",
          "required": false,
          "description": "Filter services by organization ID",
          "schema": {
            "type": "string"
          }
        },
        {
          "name": "securityLevel",
          "in": "query",
          "required": false,
          "description": "Filter flavors by security level (insecure, sensitive, secure). Flavors without model.security_level are treated as insecure. Services with no remaining flavors are excluded.",
          "schema": {
            "type": "string",
            "enum": ["insecure", "sensitive", "secure"]
          }
        }
      ],
      "responses": {
        "200": {
          "description": "The list of llm services",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ServicesLlm"
              }
            }
          }
        },
        "401": {
          "$ref": "#/components/responses/Unauthorized"
        }
      }
    }
  }
}
